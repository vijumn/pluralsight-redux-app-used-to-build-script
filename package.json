{
  "name": "ps-redux",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "scripts": {
    "start": "run-p start:dev lint:watch",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "run-p clean-dist test lint",
    "build": "webpack --config webpack.config.prod.js",
    "postbuild": "node tools/distServer.js"
  },
  "author": "Cory House",
  "engines": {
    "node": ">=8"
  },
  "license": "MIT",
  "dependencies": {
    "prop-types": "15.6.2",
    "react": "next",
    "react-dom": "next",
    "react-redux": "5.1.1",
    "react-router-dom": "next",
    "react-toastify": "4.4.3",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/register": "7.0.0",
    "autoprefixer": "9.3.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-preset-react-app": "6.1.0",
    "chalk": "2.4.1",
    "css-loader": "1.0.1",
    "cssnano": "4.1.7",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.9.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-hooks": "0.0.0",
    "eslint-watch": "4.0.2",
    "express": "4.16.4",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "0.4.5",
    "nock": "9.6.1",
    "npm-run-all": "4.1.3",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "16.6.3",
    "react-testing-library": "5.3.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "webpack": "4.26.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coryhouse/pluralsight-redux-starter"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "react",
      "react-hooks"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./tools/testSetup.js"
    ]
  }
}
